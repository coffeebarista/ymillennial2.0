'use client'

import { useState } from 'react'
import Link from 'next/link'

export default function Home() {
  const [formData, setFormData] = useState({
    fullName: '',
    phone: '',
    businessName: '',
    email: '',
    monthlyRevenue: '',
    currentPositions: '',
    industry: '',
    fundingAmount: ''
  })

  const handleInputChange = (e: React.ChangeEvent<HTMLInputElement | HTMLSelectElement>) => {
    const { name, value } = e.target
    setFormData(prev => ({
      ...prev,
      [name]: value
    }))
  }

  const handleSubmit = async (e: React.FormEvent) => {
    e.preventDefault()
    
    // Show loading state
    const submitBtn = e.currentTarget.querySelector('.submit-btn') as HTMLButtonElement
    const originalContent = submitBtn.innerHTML
    submitBtn.innerHTML = '<span>‚ö°</span> <span>Processing...</span>'
    submitBtn.disabled = true
    
    // Simulate form submission
    setTimeout(() => {
      alert('üéâ Application submitted successfully! Our team will call you within 1 hour during business hours.')
      
      // Reset form
      setFormData({
        fullName: '',
        phone: '',
        businessName: '',
        email: '',
        monthlyRevenue: '',
        currentPositions: '',
        industry: '',
        fundingAmount: ''
      })
      
      // Reset button
      submitBtn.innerHTML = originalContent
      submitBtn.disabled = false
    }, 2000)
  }

  return (
    <>
      {/* Header */}
      <header className="header" role="banner">
        <nav className="container nav" role="navigation">
          <Link href="/" className="logo">
            <div className="logo-icon">Y</div>
            <span>Millennial Funding</span>
          </Link>
          <ul className="nav-links" role="menubar">
            <li><a href="#apply">Apply Now</a></li>
            <li><a href="#contact">Contact</a></li>
          </ul>
          <a href="tel:+1-800-FUNDING" className="cta-button">üìû Direct Line</a>
        </nav>
      </header>

      <main role="main">
        {/* Hero Section */}
        <section className="hero">
          <div className="container">
            <div className="hero-content">
              <div className="hero-badge">üéØ Direct Funder ‚Ä¢ 1st-5th Position Specialist</div>
              <h1>Need <span className="highlight">Business Funding?</span></h1>
              <p className="subtitle">Up to $5M ‚Ä¢ 24-Month Terms ‚Ä¢ Fast Decisions</p>
              <p>Direct funder specializing in both first-time business loans and complex stacking arrangements for established businesses. Already have an MCA? We fund 2nd-5th positions. We fund where others won't, with same-day decisions and 24-hour funding for qualified deals.</p>
              
              <div className="qualification-bar">
                <h2>üéØ We Fund:</h2>
                <ul className="qualification-list">
                  <li>First-time business loans</li>
                  <li>1st-5th position MCA deals</li>
                  <li>Complex stacking arrangements</li>
                  <li>$50K-$5M funding amounts</li>
                  <li>All industries welcome</li>
                  <li>24-hour funding available</li>
                </ul>
              </div>
              
              <div className="hero-buttons">
                <a href="#apply" className="btn-primary">
                  <span>üöÄ</span>
                  <span>Apply For Funding</span>
                </a>
                <a href="tel:+1-800-FUNDING" className="btn-secondary">
                  <span>üìû</span>
                  <span>Speak to Our Team</span>
                </a>
              </div>
              
              <div className="hero-stats">
                <div className="stat-item">
                  <span className="stat-number">24hrs</span>
                  <span className="stat-label">Funding Speed</span>
                </div>
                <div className="stat-item">
                  <span className="stat-number">$5M</span>
                  <span className="stat-label">Max Deal Size</span>
                </div>
                <div className="stat-item">
                  <span className="stat-number">24mo</span>
                  <span className="stat-label">Max Term Length</span>
                </div>
                <div className="stat-item">
                  <span className="stat-number">1st-5th</span>
                  <span className="stat-label">Position Specialist</span>
                </div>
              </div>
            </div>
          </div>
        </section>

        {/* Application Form */}
        <section id="apply" className="apply-form">
          <div className="container">
            <div className="form-header">
              <h2>üöÄ Get Your Funding Decision Today</h2>
              <p>Complete our quick application and get your funding decision fast</p>
              <div className="qualification-notice">First-time loans & 1st-5th position deals welcome</div>
            </div>
            
            <div className="application-container">
              <form className="funding-form" onSubmit={handleSubmit}>
                <div className="form-row">
                  <div className="form-group">
                    <label htmlFor="fullName">Business Owner Name *</label>
                    <input 
                      type="text" 
                      id="fullName" 
                      name="fullName" 
                      value={formData.fullName}
                      onChange={handleInputChange}
                      required 
                    />
                  </div>
                  <div className="form-group">
                    <label htmlFor="phone">Direct Phone Number *</label>
                    <input 
                      type="tel" 
                      id="phone" 
                      name="phone" 
